<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>

    <env-entry>
        <env-entry-name>db-host</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>localhost</env-entry-value>
    </env-entry>

    <env-entry>
        <env-entry-name>db-login</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>root</env-entry-value>
    </env-entry>

    <env-entry>
        <env-entry-name>db-pwd</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>root</env-entry-value>
    </env-entry>

    <env-entry>
        <env-entry-name>db-port</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>3306</env-entry-value>
    </env-entry>

    <env-entry>
        <env-entry-name>db-name</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>vide_dressing</env-entry-value>
    </env-entry>

</web-app>